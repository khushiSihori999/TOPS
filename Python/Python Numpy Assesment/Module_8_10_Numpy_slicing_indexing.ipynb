        "import numpy as np\n",
        "\n",
        "arr = np.array([1, 2, 3, 4, 5])\n",
        "print(arr[1:4])  # Output: [2 3 4]\n",
        "print(arr[:3])   # Output: [1 2 3]\n",
        "print(arr[::2])  # Output: [1 3 5]"
